                            <dxe:CalcEditSettings DisplayFormat="n2" AllowDefaultButton="False"
                                                          Mask="n2" IsTextEditable="False"/>
                        </dxg:GridColumn.EditSettings>
                    </dxg:GridColumn>
                    
                </dxg:GridControlBand>
                <dxg:GridControlBand Name="bandgr_1" Header="RUB">
