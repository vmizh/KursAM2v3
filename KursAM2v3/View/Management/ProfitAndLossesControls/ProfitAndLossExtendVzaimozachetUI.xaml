                                                            SortMode="DisplayText"  ReadOnly="True"/>
                            <dxg:GridColumn Name="colz4" FieldName="CurrencyName" Header="Валюта"  ReadOnly="True"/>
                            <dxg:GridColumn Name="colz5" FieldName="Summa" Header="Сумма"  ReadOnly="True">
                                <dxg:GridColumn.EditSettings>
                                    <dxe:CalcEditSettings DisplayFormat="n2" AllowDefaultButton="False"
                                                                          Mask="n2" />
                                </dxg:GridColumn.EditSettings>
                            </dxg:GridColumn>
                            <dxg:GridColumn Name="colz6" FieldName="Note" Header="Примечание"  ReadOnly="True"/>
                            </dxg:GridControlBand>
                            <dxg:GridControlBand Name="xbandgr_1" Header="RUB">
                                <dxg:GridColumn Name="xcol4" FieldName="ProfitRUB" Header="Доход" ReadOnly="True">
                                    <dxg:GridColumn.EditSettings>
                                        <dxe:CalcEditSettings DisplayFormat="n2" AllowDefaultButton="False"
                                                          Mask="n2" />
                                    </dxg:GridColumn.EditSettings>
                                </dxg:GridColumn>
                                <dxg:GridColumn Name="xcol5" FieldName="LossRUB" Header="Расход" ReadOnly="True">
                                    <dxg:GridColumn.EditSettings>
                                        <dxe:CalcEditSettings DisplayFormat="n2" AllowDefaultButton="False"
                                                          Mask="n2" />
