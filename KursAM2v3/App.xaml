<Application x:Class="KursAM2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             StartupUri="View/WindowStart.xaml"
             DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource\StandartIcon.xaml" />
                <ResourceDictionary Source="Resource\DialogResorces.xaml"/>
                <ResourceDictionary Source="Resource\StandartButtons.xaml" />
                <ResourceDictionary Source="Resource\Menu.xaml" />
                <ResourceDictionary Source="Resource\Error.xaml" />
                <ResourceDictionary Source="Resource\ViewControlTemplate.xaml" />
                <ResourceDictionary Source="Resource\Window.xaml" />
                <ResourceDictionary Source="Resource\DXControlStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Button.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
